{
    "version": 2,
    "builds": [
        {
            "src": "server.cjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.cjs",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS",
                "HEAD"
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}